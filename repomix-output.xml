/* Import the Orbitron font */
@import url("https://fonts.googleapis.com/css2?family=Orbitron:wght@400..900&display=swap");

/* Define the original color palette */
body {
  font-family: "Orbitron", sans-serif;

  --ion-background-color: #1e3a8a; /* The original deep blue */
  --ion-text-color: #ffffff;
  --ion-color-primary: #3b82f6;
  --ion-color-primary-contrast: #ffffff;

  /* Make the dark theme toolbar match our background */
  --ion-toolbar-background: #1e3a8a;

  /* Lighter grey for secondary text */
  --ion-color-medium: #d1d5db;
}

ion-toolbar {
  --background: rgba(
    0,
    0,
    0,
    0.2
  ); /* Semi-transparent background like the footer */
  --border-width: 0px; /* Remove default border */
  backdrop-filter: blur(10px);
  -webkit-backdrop-filter: blur(10px);
  border-bottom: 1px solid rgba(255, 255, 255, 0.1); /* Add our custom border */
}

/* Custom Card Styles */
ion-card {
  --background: transparent;
}

ion-item {
  --background: transparent;
  --border-color: rgba(255, 255, 255, 0.2);
}

ion-content {
  --background: var(--ion-background-color);
}

/* Run Action Sheet Styles */

.run-action-sheet {
  --backdrop-opacity: 0.6;
}

.run-action-sheet .action-sheet-title {
  color: #e0e0e0;
  font-weight: 600;
}

.run-action-sheet .action-sheet-button {
  color: #ffffff;
}

.run-action-sheet .action-sheet-button.action-sheet-cancel {
  color: #ff8a80;
  font-weight: bold;
}
